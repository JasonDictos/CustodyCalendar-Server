{
  "name": "custodycalendar",
  "version": "1.0.0",
  "module": "esNext",
  "description": "A way to manage custody",
  "type": "module",
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "jest --coverage",
    "test:lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "fmt": "npx tsfmt -r",
    "build": "tsc -p tsconfig.app.json",
    "watch:build": "tsc --watch",
    "watch:test": "jest --watch"
  },
  "files": [
    "build/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonDictos/CustodyCalendar.git"
  },
  "author": "Jason Dictos",
  "license": "commercial",
  "bugs": {
    "url": "https://github.com/JasonDictos/CustodyCalendar/issues"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "homepage": "https://github.com/JasonDictos/CustodyCalendar#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^59.0.0",
    "i": "^0.3.6",
    "knex": "^0.95.2",
    "luxon": "^1.24.1",
    "module-alias": "^2.2.2",
    "moment": "^2.27.0",
    "parse-database-url": "^0.3.0",
    "pg": "^8.5.1",
    "sqlite3": "^5.0.2",
    "ts-node": "^9.1.1"
  },
  "keywords": [
    "calendar",
    "custody"
  ],
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.21",
    "@types/knex": "^0.16.1",
    "@types/luxon": "^1.26.2",
    "@types/node": "^14.14.35",
    "jest": "^26.6.3",
    "ramda": "^0.27.1",
    "ts-jest": "^26.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "typescript-formatter": "^7.2.2"
  }
}
